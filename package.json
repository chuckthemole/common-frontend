{
    "name": "@rumpushub/common-react",
    "version": "1.1.2",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "babel src --out-dir dist --copy-files && npm run copy:css",
        "copy:css": "cp -r src/components/dashboard-elements/*/styles/* dist/components/dashboard-elements/",
        "build:watch": "babel src --out-dir dist --watch",
        "clean": "echo Cleaning frontend && rm -rf node_modules package-lock.json",
        "fresh": "npm run clean && npm install",
        "check-global-links": "npm list -g --depth=0 --link=true",
        "build:scss": "sass src/components/dashboard-elements:dist/components/dashboard-elements --no-source-map"
    },
    "peerDependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "aws-sdk": "^2.0.0",
        "axios": "^1.10.0",
        "consola": "^8.0.0",
        "lucide-react": "^0.525.0",
        "qs": "^6.14.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-modal": "^3.0.0",
        "react-quill": "^2.0.0",
        "react-router-dom": "^6.0.0",
        "react-tooltip": "^5.0.0",
        "sass": "^1.89.2",
        "swr": "^2.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.0",
        "@babel/core": "^7.22.0",
        "@babel/preset-env": "^7.22.0",
        "@babel/preset-react": "^7.22.0",
        "consola": "^3.4.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "webpack-cli": "^5.1.4"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "publishConfig": {
        "access": "restricted"
    }
}